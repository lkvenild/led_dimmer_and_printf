<?xml version="1.0" encoding="utf-8"?>
<package schemaVersion="1.3" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="PACK.xsd">
	<name>Application_AVR_Examples</name>
	<vendor>Atmel</vendor>
	<description>This is a PDSC created to make samples with ACME framework.</description>
	<url>http://www.atmel.com</url>
	<releases>
		<release version="1.0.0">init</release>
	</releases>
	<examples>
    	<example name="LEDdimmer817" folder="led_dimmer_and_printf" doc="led_dimmer_and_printf_817.rst" version="1.0.0">
			<description>This example uses the ADC to make a reading, converts the reading to a PWM 
			duty-cycle used to dim a LED. printf() output is routed to EDBG USART. </description>
			<board vendor="Atmel" name="ATtiny817XplainedPro"/>
			<project>
				<environment name="gcc" load="Makefile" />
				<environment name="atmel_start" load="led_dimmer_and_printf/tiny817/led_dimmer_and_printf.yaml"/>
			</project>
			<attributes>
				<component Cclass="Application" Cgroup="Basic Peripherals tiny817"/>
				<category>I/O</category>
				<category>Calendar</category>
				<category>Analog</category>
				<category>Sensors</category>
				<category>Timers</category>
			</attributes>
		</example>
	</examples>
</package>        